{
    "env": {
        "node": true,
        "browser": true
    },
    "extends": ["airbnb-base"],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": ["react", "prettier"],
    "rules": {
        "import/prefer-default-export": 0,
        "no-underscore-dangle": 0,
        "react/prop-types": 0,
        "no-unused-vars": 0,
        "no-nested-ternary": 0,
        "react/function-component-definition": [2, "arrowFunction"],
        "jsx-props-no-spreading": "false"
    }
}
